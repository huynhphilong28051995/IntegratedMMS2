<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>invalidLogin</servlet-name>
        <jsp-file>/invalidLogin.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ViewRentInformation</servlet-name>
        <jsp-file>/ViewRentInformation.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <jsp-file>/home.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>TenantSystemServlet</servlet-name>
        <servlet-class>servlet.TenantSystemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OutsourceRequest</servlet-name>
        <jsp-file>/OutsourceRequest.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>OutsourceStatus</servlet-name>
        <jsp-file>/OutsourceStatus.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>RentRenewalRequest</servlet-name>
        <jsp-file>/RentRenewalRequest.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>RentRenewalStatus</servlet-name>
        <jsp-file>/RentRenewalStatus.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ThankYou</servlet-name>
        <jsp-file>/ThankYou.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>RentInvoice</servlet-name>
        <jsp-file>/RentInvoice.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ViewSales</servlet-name>
        <jsp-file>/ViewSales.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>OverallSales</servlet-name>
        <jsp-file>/OverallSales.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>SalesChart</servlet-name>
        <jsp-file>/SalesChart.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>invalidLogin</servlet-name>
        <url-pattern>/invalidLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewRentInformation</servlet-name>
        <url-pattern>/ViewRentInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TenantSystemServlet</servlet-name>
        <url-pattern>/TenantSystemServlet/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OutsourceRequest</servlet-name>
        <url-pattern>/OutsourceRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OutsourceStatus</servlet-name>
        <url-pattern>/OutsourceStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentRenewalRequest</servlet-name>
        <url-pattern>/RentRenewalRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentRenewalStatus</servlet-name>
        <url-pattern>/RentRenewalStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThankYou</servlet-name>
        <url-pattern>/ThankYou</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentInvoice</servlet-name>
        <url-pattern>/RentInvoice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewSales</servlet-name>
        <url-pattern>/ViewSales</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OverallSales</servlet-name>
        <url-pattern>/OverallSales</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalesChart</servlet-name>
        <url-pattern>/SalesChart</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
